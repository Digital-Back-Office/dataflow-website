---
import Layout from '../layouts/Layout.astro'
import TopSection from '../components/apps/TopSection.astro'

import { getEntry } from 'astro:content'
import AppSection from '../components/apps/AppSection.astro'

const Data = await getEntry('apps', 'apps')
const data1 = Data.data.section1.title
const data2 = Data.data.section2.title
const data3 = Data.data.section3.title
const data4 = Data.data.section4.title
const data5 = Data.data.section5.title
const data6 = Data.data.section6.title
const data7 = Data.data.section7.title
const data8 = Data.data.section8.title
---

<Layout title='Dataflow | Apps'>
  <TopSection />
  <AppSection
    title={data1}
    image={'/assets/images/apache-airflow-web.png'}
    logo={'/assets/images/airflow-logo.svg'}
    link={'https://airflow.apache.org/'}
  />
  <AppSection
    title={data2}
    mirror
    image={'/assets/images/jupyterhub-web.png'}
    logo={'/assets/images/jupyterlogofull.png'}
    link={'https://jupyter.org/'}
  />
  <AppSection
    title={data3}
    image={'/assets/images/streamlit-web.png'}
    logo={'/assets/images/streamlit-logo.png'}
    link={'https://streamlit.io/'}
  />
  <AppSection
    title={data4}
    mirror
    image={'/assets/images/mlflow-web.png'}
    logo={'/assets/images/mlflowlogofull.png'}
    link={'https://mlflow.org/'}
  />

  <AppSection
    title={data6}
    image={'/assets/images/vscode-web.png'}
    logo={'/assets/images/vs-code-logo.png'}
    link={'https://code.visualstudio.com/'}
  />
