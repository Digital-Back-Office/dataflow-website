---
import Layout from "../layouts/Layout.astro";
import Leftcard from "../components/Learn/Leftcard.astro";
import Rightcard from "../components/Learn/Rightcard.astro";
import TopSection from "../components/Learn/TopSection.astro";

import { getEntry } from "astro:content";

const Data = await getEntry("learn", "learn");
const data1 = Data.data.section1.title;
const data2 = Data.data.section2.title;
const data3 = Data.data.section3.title;
const data4 = Data.data.section4.title;
const data5 = Data.data.section5.title;
const data6 = Data.data.section6.title;
const data7 = Data.data.section7.title;
const data8 = Data.data.section8.title;


---

<Layout title="Learn More">

    <TopSection/>
    <Leftcard image1="/assets/images/learnmoresection1left.png" image2="/assets/images/learnmoresection1right.png" logo="/assets/images/Airflowlogofull.png" title={data1}/>
    <Rightcard image1="/assets/images/learnmoresection2left.png" image2="/assets/images/learnmoresection2right.png" logo="/assets/images/jupyterlogofull.png" title={data2}/>
    <Leftcard image1="/assets/images/learnmoresection3left.png" image2="/assets/images/learnmoresection3right.png" logo="/assets/images/sparklogofull.png" title={data3}/>
    <Rightcard image1="/assets/images/learnmoresection4left.png" image2="/assets/images/learnmoresection4right.png" logo="/assets/images/mlflowlogofull.png" title={data4}/>
    <Leftcard image1="/assets/images/learnmoresection5left.png" image2="/assets/images/learnmoresection5right.png" logo="/assets/images/pytorchlogofull.png" title={data5}/>
    <Rightcard image1="/assets/images/learnmoresection6left.png" image2="/assets/images/learnmoresection6right.png" logo="/assets/images/theiaidelogofull.png" title={data6}/>
    <Leftcard image1="/assets/images/learnmoresection7left.png" image2="/assets/images/learnmoresection7right.png" logo="/assets/images/dasklogofull.png" title={data7}/>
    <Rightcard image1="/assets/images/learnmoresection8left.png" image2="/assets/images/learnmoresection8right.png" logo="/assets/images/dbtlogofull.png" title={data8}/>
  
   
</Layout>
