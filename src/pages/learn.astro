---
import Layout from "../layouts/Layout.astro";
import Leftcard from "../components/Learn/Leftcard.astro";
import Rightcard from "../components/Learn/Rightcard.astro";
import TopSection from "../components/Learn/TopSection.astro";

import { getEntry } from "astro:content";

const Data = await getEntry("learn", "learn");
const data1 = Data.data.section1.title;
const data2 = Data.data.section2.title;
const data3 = Data.data.section3.title;
const data4 = Data.data.section4.title;
const data5 = Data.data.section5.title;
const data6 = Data.data.section6.title;
const data7 = Data.data.section7.title;
const data8 = Data.data.section8.title;


---

<Layout title="LearnMore">

    <TopSection/>
    <Leftcard image1="/assets/images/learnmoresection1left.png" image2="/assets/images/learnmoresection1right.png" logo="/assets/images/Airflowlogofull.png" title={data1} link="https://airflow.apache.org/" />
    <Rightcard image1="/assets/images/learnmoresection2left.png" image2="/assets/images/learnmoresection2right.png" logo="/assets/images/jupyterlogofull.png" title={data2} link="https://jupyter.org/" />
    <Leftcard image1="/assets/images/learnmoresection3left.png" image2="/assets/images/learnmoresection3right.png" logo="/assets/images/sparklogofull.png" title={data3} link="https://spark.apache.org/" />
    <Rightcard image1="/assets/images/learnmoresection4left.png" image2="/assets/images/learnmoresection4right.png" logo="/assets/images/mlflowlogofull.png" title={data4} link="https://mlflow.org/" />
    <Leftcard image1="/assets/images/learnmoresection5left.png" image2="/assets/images/learnmoresection5right.png" logo="/assets/images/pytorchlogofull.png" title={data5} link="https://pytorch.org/" />
    <Rightcard image1="/assets/images/learnmoresection6left.png" image2="/assets/images/learnmoresection6right.png" logo="/assets/images/theiaidelogofull.png" title={data6} link="https://theia-ide.org/" />
    <Leftcard image1="/assets/images/learnmoresection7left.png" image2="/assets/images/learnmoresection7right.png" logo="/assets/images/dasklogofull.png" title={data7} link="https://www.dask.org/" />
    <Rightcard image1="/assets/images/learnmoresection8left.png" image2="/assets/images/learnmoresection8right.png" logo="/assets/images/dbtlogofull.png" title={data8} link="https://www.getdbt.com/product/what-is-dbt" />
  
   
</Layout>
