---
import Layout from '../layouts/Layout.astro'
import TopSection from '../components/RunDataAnyCloud/TopSection.astro'
import Section1 from '../components/RunDataAnyCloud/Section1.astro'
import Section2 from '../components/RunDataAnyCloud/Section2.astro'
import Section6 from '../components/RunDataAnyCloud/Section6.astro'
import { getEntry } from 'astro:content'
import Section from '../components/RunDataAnyCloud/Section.astro'
const Data = await getEntry('features', 'run-data-on-any-cloud')
const data1 = Data.data.Section3
const data2 = Data.data.Section4
const data3 = Data.data.Section5
---

<Layout title='Run Data on any cloud'>
  <TopSection />
  <Section1 />
  <Section2 />
  <Section
    logo='/assets/images/azurelogo.png'
    title={data1.title}
    image='/assets/images/macbookazure.png'
    link='https://azure.microsoft.com/'
  />
  <Section
    mirror
    logo='/assets/images/googlecloudlogo.png'
    title={data2.title}
    image='/assets/images/macbookgooglecloud.png'
    link='https://cloud.google.com/'
  />
  <Section
    logo='/assets/images/aws-logo.png'
    title={data3.title}
    image='/assets/images/macbookaws.png'
    link='https://aws.amazon.com/'
  />
  <Section6 />
</Layout>
